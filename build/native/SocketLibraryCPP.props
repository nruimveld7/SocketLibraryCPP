<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="SocketLibraryCPP">
    <SocketLibraryCPP_IncludeDir Condition="'$(SocketLibraryCPP_IncludeDir)' == ''">
      $(MSBuildThisFileDirectory)include
    </SocketLibraryCPP_IncludeDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>
        $(SocketLibraryCPP_IncludeDir);%(AdditionalIncludeDirectories)
      </AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>
